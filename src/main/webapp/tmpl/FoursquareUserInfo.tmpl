<script id="tmpl-FoursquareUserInfo" type="text/html">
	<div class="container FoursquareUserInfo"> 
		<table class="table table-striped">
			<tr>
				<td>Email</td>
				<td>{{contact.email}}</td>
			</tr>			<tr>
				<td>User Name:</td>
				<td>{{lastName}}.{{firstName}}</td>
			</tr>
            <tr>
            <td>Avatar:</td>
                <td><image src="{{photo.prefix}}64x64{{photo.suffix}}" class="avatar"/></td>
            </tr>
            <td>Gender:</td>
                <td>{{gender}}</td>
            </tr>
            <tr>
            	<td>Friends:</td>
				<td>{{friends.count}}</td>
			</tr>
			<tr>
				<td>Checkins :</td>
				<td>{{checkins.count}}</td>
			</tr>
			<tr>
				<td>badges :</td>
				<td>{{badges.count}}</td>
			</tr>
		</table>
	</div>
</script>