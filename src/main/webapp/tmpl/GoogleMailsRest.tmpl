<script id="tmpl-GoogleMailsRest" type="text/html">
	<div class="GoogleMailsRest">
		<div><span class="btn btn-default searchCondition">Search condition</span></div>
		<div class="search-mails-container">
			<div class="control-group">
				Subject: <input type="text"  name="subject"  placeholder="Subject value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				From: <input type="text"  name="from"  placeholder="From value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				To: <input type="text"  name="to"  placeholder="To value">
				<span class="help-inline" ></span>
			</div>

			<div class="control-group">
				Body: <input type="text"  name="body"  placeholder="Body value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Min Mail Size: <input type="text"  name="minSize"  placeholder="Like: 10000 or 1M">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Max Mail Size: <input type="text"  name="maxSize"  placeholder="Like: 10000 or 1M">
				<span class="help-inline" ></span>
			</div>

			<div class="control-group">
				Start Received Date
				<div class="input-append datetimepicker date">
					<input type="text" name = "startReceivedDate" placeholder="Click to select">
					<span class="add-on">
						<i class="glyphicon glyphicon-calendar"></i>
					</span>
				</div>
			</div>
			<div class="control-group">
				End Received  Date
				<div class="input-append datetimepicker date">
					<input type="text" name = "endReceivedDate" placeholder="Click to select">
					<span class="add-on">
						<i class="glyphicon glyphicon-calendar"></i>
					</span>
				</div>
			</div>
			<div class="control-group">
				Label: <input type="text"  name="label"  placeholder="Label value">
				<span class="help-inline" ></span>
			</div>

			<div class="control-group">
				<span class="leftLabel">Has Attachment:</span> 
				<input type="checkbox"  class="checkbox" name="hasAttachment" ><span class="help-inline" ></span>
			</div>
			<div class="control-group">
				<span class="leftLabel">Has Circle:</span> 
				<input type="checkbox" class="checkbox"  name="hasCircle" ><span class="help-inline" ></span>
			</div>
			<div class="control-group">
				<span class="leftLabel">Is Unread:</span> 
				<input type="checkbox" class="checkbox"  name="unread" ><span class="help-inline" ></span>
			</div>

			<div class="control-group">
				Attachment Name: <input type="text"  name="attachmentName"  placeholder="Attachment Name value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Circle Name: <input type="text"  name="circle"  placeholder="Circle Name value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				CC: <input type="text"  name="cc"  placeholder="CC value">
				<span class="help-inline" ></span>
			</div>

			<div class="control-group">
				List: <input type="text"  name="list"  placeholder="List value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Chat Content: <input type="text"  name="chatContent"  placeholder="Chat Content value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Category: <input type="text"  name="category"  placeholder="Category value">
				<span class="help-inline" ></span>
			</div>

			<div class="control-group">
				Delivered To: <input type="text"  name="deliveredTo"  placeholder="Delivered To value">
				<span class="help-inline" ></span>
			</div>
			<div class="control-group">
				Rfc822msgid: <input type="text"  name="rfc822msgid"  placeholder="Rfc822msgid value">
				<span class="help-inline" ></span>
			</div>

			<div>
				<a href="#" class="btn btn-default inputValueBtn">Search</a> 
				<a href="#" class="btn btn-default cleanSearchBtn">Clean Search</a>
			</div>
		</div>

		<div class="mails-folder">Folder Name: <span class="folderName"></span></div>
		<div class="mails-container"></div>
	</div>
</script>